 <animation>
  <type>select</type>
  <object-name>LMainGear</object-name>
  <object-name>RMainGear</object-name>
  <object-name>LMainTire</object-name>
  <object-name>RMainTire</object-name>
  <object-name>LMainGearDoorUpper</object-name>
  <object-name>LMainGearDoorLower</object-name>
  <object-name>LMainGearDoorFwd</object-name>
  <object-name>RMainGearDoorUpper</object-name>
  <object-name>RMainGearDoorLower</object-name>
  <object-name>RMainGearDoorFwd</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[1]/position-norm</property>
    <value>0.02</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseGearStrutUpper</object-name>
  <object-name>NoseGearStrutDoor</object-name>
  <object-name>NoseGearForkUpper</object-name>
  <object-name>NoseGearForkLower</object-name>
  <object-name>NoseGearOleo</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.05</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseWellCoverFwd</object-name>
  <condition>
   <or>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.95</value>
   </greater-than>
   <less-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.02</value>
   </less-than>
   </or>
  </condition>
 </animation>

